    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "reduce-reducers": "^1.0.4",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-form": "8.2.4"
  },
